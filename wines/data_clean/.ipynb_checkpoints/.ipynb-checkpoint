    "# Converting Kč into EUR"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 465,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(0, size_clean):\n",
    "    if 'Kč' in wines_clean['avg_price'].iloc[i]:\n",
    "        j = wines_clean['avg_price'].iloc[i]\n",
    "        j = j.replace('Kč', '')\n",
    "        j = float(j) * 0.039\n",
    "        j = str(j)\n",
    "        wines_clean['avg_price'].iloc[i] = j"
   ]
  },
