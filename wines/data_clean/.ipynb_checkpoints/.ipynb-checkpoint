   "source": [
    "wines_clean = wines_clean.replace({'\\n':''}, regex = True)\n",
    "wines_clean['avg_price'] = wines_clean['avg_price'].replace({'\\$':''},regex = True)\n",
    "wines_clean['avg_price'] = wines_clean['avg_price'].replace({'\\,':''},regex = True)\n",
    "wines_clean['avg_price'] = wines_clean['avg_price'].replace({'\\ ':''},regex = True)"
   ]
