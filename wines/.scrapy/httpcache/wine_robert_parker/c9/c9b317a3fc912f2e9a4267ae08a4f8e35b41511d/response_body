            var fix_url = 'https://www.wine-searcher.com/sitefix.lml';
            var documenturl = document.URL;
            if (documenturl == ''){documenturl = fix_url;}
            if (document.getElementById('sitefix') != null){
              document.getElementById("sitefix").href = fix_url+'?url_F='+encodeURIComponent(documenturl);
            }
          </script>
