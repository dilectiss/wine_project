<script type="text/javascript">           
                      var el = document.getElementById('crt_6');
                      var options = {
                          percent: el.getAttribute('data-percent'),
                          percent_high: el.getAttribute('data-percent-high'),
                          percent_low: el.getAttribute('data-percent-low'),
                          percent_base: el.getAttribute('data-percent-base'),
                          percent_hidden: el.getAttribute('data-hidden'),
                          size: el.getAttribute('data-size'),
                          lineWidth: el.getAttribute('data-line'),
                          rotate: el.getAttribute('data-rotate')
                      }
                      var c = document.getElementById('crtCanvas_6');  
                      var ctx = c.getContext('2d');
                      c.width = c.height = options.size;
                      ctx.translate(options.size / 2, options.size / 2); 
                      ctx.rotate((-1 / 2 + options.rotate / 180) * Math.PI);
                      var r = (options.size - options.lineWidth) / 2;
                      var drawCircle = function(color, lineWidth, percent) {
                        percent = Math.min(Math.max(0, percent || 1), 1);
                        ctx.beginPath();
                        ctx.arc(0, 0, r, 0, Math.PI * 2 * percent, false);
                        ctx.strokeStyle = color;
                        ctx.lineWidth = lineWidth
                        ctx.stroke();
                      };
                      drawCircle('#efefef', options.lineWidth, 100 / 100);
                      if(options.percent_hidden=="Y"){
                        drawCircle('#8e1150', options.lineWidth, 1 / 1);
                      }else{
                        drawCircle('#a9547c', options.lineWidth, options.percent_high / options.percent_base); 
                        drawCircle('#8e1150', options.lineWidth, options.percent_low / options.percent_base);
                      }
                    </script> </div>
