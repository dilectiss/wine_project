<div><script type="text/javascript">
  
    var customConfigId = '2620067387';    
    var javasriptResourceUrl = 'https://ui.customsearch.ai/api/ux/render?customConfig=2620067387&market=en-US&safeSearch=Moderate';
    var s = document.createElement('script');
    s.setAttribute('type', 'text/javascript');        
    s.defer = true;
    s.id = 'bcs_js_snippet';
    s.src = javasriptResourceUrl;
    var scripts = document.getElementsByTagName("script"),
    currentScript = scripts[scripts.length-1];
    currentScript.parentElement.appendChild(s);

    var searchTermLoaded = false;
    
    function setBCSsearchTerm() {
      var cnt = 0;
      var id = setInterval(chkLoading, 500);
      function chkLoading() {
        cnt++;
        var es = document.getElementsByClassName('bcs-searchbox');
        var le = es.length;
        if (le == 1 || cnt == 10) {
          clearInterval(id);
        }
        if (le == 1) {
          var imgURL = 'https://cdn1.wine-searcher.net/images';
          var imgName = '/ajax-loader-small.gif';
          es[0].value = 'Chateau Margaux, Margaux, France 2015';
          $('.bcs-host-div').append('<div id="bcs-results-loading" style="text-align:center;padding-bottom:5px;display:none;"><img src="' + imgURL + imgName + '"></div>');
          searchTermLoaded = true;
        }
      }
    }
    setBCSsearchTerm();
    
    function bcsLoadingProgress() {
      $('#bcs-results-loading').css('display','block');
      var id = setInterval(removeLoading, 100);
      function removeLoading() {
        if (document.getElementById('bcs-results-loading').style.display == 'none') {
          clearInterval(id);
        }
        else if (document.getElementById('bcs-results-div')) {
          document.getElementById('bcs-results-loading').style.display = 'none';
          clearInterval(id);
        }
      }
    }
    
    _labq.push(function() {
      var doBCSsearch = true;
      $(document).ready(function() {
        $('#showTastingNote').click(function() {
          if (searchTermLoaded) {
            if ($('#showTastingNote').html() == 'View') {
              $('#showTastingNote').html('Hide');
              $('#bcs-results-div').css('display','block');
              if (doBCSsearch) {
                bcsSearchBoxSubmit();
                bcsLoadingProgress();
                doBCSsearch = false;
              }
            }
            else {
              $("#showTastingNote").html("View");
              $('#bcs-results-div').css('display','none');
            }
          }
        });
      });
    });
  </script></div>
