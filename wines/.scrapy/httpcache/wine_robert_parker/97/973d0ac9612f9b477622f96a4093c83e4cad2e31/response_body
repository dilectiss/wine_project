<meta itemprop="ratingValue" content="100">
<meta itemprop="bestRating" content="100"> <div class="critic-score-cirle-s" id="crt_7" data-percent="100" data-hidden="" data-percent-base="100" data-percent-high="100" data-percent-low="100" data-size="60" data-line="3" data-rotate="0">
